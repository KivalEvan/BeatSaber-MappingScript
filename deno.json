{
   "lock": false,
   "importMap": "./import_map.json",
   "lint": {
      "exclude": ["chromapper"]
   },
   "fmt": {
      "exclude": ["chromapper"],
      "indentWidth": 3,
      "lineWidth": 100,
      "singleQuote": true
   },
   "tasks": {
      "environment": "deno run --allow-read=. --allow-write=. ./environment-enhancement/main.ts",
      "map": "deno run --allow-read --allow-write --allow-run --allow-net ./map/main.ts",
      "map:watch": "deno run --allow-read --allow-write --allow-net --allow-run ./map/main.ts --watch"
   }
}
