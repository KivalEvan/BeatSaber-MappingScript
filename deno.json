{
   "lock": false,
   "lint": {
      "exclude": ["chromapper"]
   },
   "fmt": {
      "exclude": ["chromapper"],
      "indentWidth": 3,
      "lineWidth": 100,
      "singleQuote": true
   },
   "tasks": {
      "environment": "deno run --allow-read=. --allow-write=. --allow-hrtime ./environment-enhancement/main.ts",
      "map": "deno run --allow-read --allow-write --allow-hrtime --allow-run --allow-net ./map/main.ts",
      "map:watch": "deno run --allow-read --allow-write --allow-hrtime --allow-net --allow-run --watch ./map/main.ts"
   }
}
